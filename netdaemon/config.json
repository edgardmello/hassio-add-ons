{
  "name": "NetDaemon",
  "version": "0.0.32",
  "slug": "netdaemon",
  "description": "Application daemon written in .NET core 3.1",
  "url": "https://github.com/helto4real/hassio-add-ons/tree/master/netdaemon",
  "startup": "application",
  "image": "helto4real/hassio-netdaemon-{arch}",
  "arch": ["aarch64", "amd64", "armhf", "armv7"],
  "boot": "auto",
  "map": ["config:rw"],
  "hassio_api": true,
  "hassio_role": "default",
  "homeassistant_api": true,
  "host_network": false,
  "options": {
    "log_level": "info"
  },
  "schema": {
    "log_level": "match(^(trace|debug|info|warning|error)$)"
  }
}
